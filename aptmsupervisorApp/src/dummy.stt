ss ss1 {

    state state_one 
	{
		entry
		{
			printf("We entered state one\n");
			myPV2 = 5;			
			pvPut(myPV2);
		}        
		
		when (delay(5)) 
		{
        } state state_two
	} 

	state state_two
	{
